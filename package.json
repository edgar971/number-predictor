{
  "name": "number-recognition",
  "version": "1.0.0",
  "description": "A simple number predictor built using a Tensorflow.js model trained on the MNIST dataset.",
  "author": {
    "name": "Edgar Pino"
  },
  "repository": "https://github.com/edgar971/number-predictor",
  "homepage": "https://edgar971.github.io/number-predictor",
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "format": "prettier --write src/{,**/}/*.{ts*,js,jsx,css}",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build"
  },
  "dependencies": {
    "@types/victory": "^0.9.16",
    "gh-pages": "^1.2.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-scripts-ts": "2.16.0",
    "victory": "^0.27.2"
  },
  "devDependencies": {
    "@tensorflow/tfjs": "^0.11.7",
    "@types/jest": "^23.1.1",
    "@types/node": "^10.3.4",
    "@types/react": "^16.4.1",
    "@types/react-dom": "^16.0.6",
    "prettier": "1.13.6",
    "typescript": "^2.9.2"
  }
}